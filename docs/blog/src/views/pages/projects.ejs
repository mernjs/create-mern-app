							<div class="row">
								<div class="col-12">
                  <!-- Recent Order Table -->
                  <div class="card card-table-border-none" id="recent-orders">
                    <div class="card-header card-header-border-bottom">
                      <h2>All Projects</h2>
                      
                    </div>
                    <div class="card-body pt-0 pb-5">
                      <table class="table card-table table-responsive table-responsive-large" style="width:100%">
                        <thead>
                          <tr>
                            <th>SN</th>
                            <th>Project Name</th>
                            <th class="d-none d-md-table-cell">Project Type</th>
                            <th class="d-none d-md-table-cell">Version</th>
                            <th class="d-none d-md-table-cell">Email</th>
                            <th>Date</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% data.forEach(function(item, index){ %>
                          <tr>
                            <td><%= index+1 %></td>
                            <td>
                              <a class="text-dark" href="project-details?project_id=<%= item._id %>"> <%= item.project_name %></a>
                            </td>
                            <td width="250px" class="d-none d-md-table-cell"><%= item.project_type %></td>
                            <td class="d-none d-md-table-cell"><%= item.cli_version %></td>
                            <td class="d-none d-md-table-cell"><%= item.email %></td>
                            <td><%= moment(item.created_at).format('DD-MM-YYYY') %></td>
                          </tr>
                          <% }) %>

                        </tbody>
                      </table>
                    </div>
                  </div>
</div>
